<beast version='2.0' namespace='beast.base.inference.parameter:beast.base.inference:remaster'>

    <run spec="Simulator" nSims="15">

        <simulate id="tree" spec="SimulatedTree">
            <trajectory id="trajectory"
                        spec="StochasticTrajectory"
                        maxTime="35"
                        mustHave="Psi>2">

                <population id="X" spec="RealParameter" value="1" />
                <samplePopulation id="Psi" spec="RealParameter" value="0" />
                <population id="Mu" spec="RealParameter" value="0" />
                <population id="Omega" spec="RealParameter" value="0" />

                <reaction spec="Reaction" rate="0.185">X -> 2X</reaction>
                <reaction spec="Reaction" rate="0.008">X -> Psi</reaction>
                <reaction spec="Reaction" rate="0.046">X -> Mu</reaction>
                <reaction spec="Reaction" rate="0.046">X -> Omega</reaction>

                <reaction spec="PunctualReaction" p="0.5" times="35">X -> Psi</reaction>

            </trajectory>
        </simulate>

        <logger fileName="out/s1/remaster-scenario-1.tree" mode="tree">
            <log spec="TypedTreeLogger" tree="@tree" />
        </logger>

        <logger fileName="out/s1/remaster-scenario-1.log">
            <log idref="trajectory" />
        </logger>
    </run>
</beast>
